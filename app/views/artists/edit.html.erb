<h1>Edit Artist</h1>

<%= render('layouts/errors', :object => @artist) %>

<form action="/artists/<%= @artist.id %>" method="post">
  <input name="_method" type="hidden" value="patch">
  <%= render('form') %>
  <select name="tags">
    <% @tags.each do |tag| %>
      <option value="<%= tag.id %>"><%= tag.name %></option>
    <% end %>
  </select>
  <button>Update Artist</button>
</form>
